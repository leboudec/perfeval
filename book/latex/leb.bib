@INPROCEEDINGS{qofis2000,
AUTHOR="Charny, A. and Le Boudec, J.-Y.",
TITLE="Delay Bounds in a Network With Aggregate Scheduling",
BOOKTITLE="First International Workshop on Quality of future Internet Services",
ADDRESS="Berlin, Germany",
  publisher = {Springer},
  series    = {Lecture Notes in Computer Science},
  volume    = {1922},
  pages     = {1--13},
YEAR=2000,
MONTH="September",
ABSTRACT="We study bounds on the worst case delay in a network implementing
aggregate scheduling. We derive an upper bound on the queuing
delay as a function of priority traffic utilization and the
maximum hop count of any flow, and the shaping parameters at the
network ingress. Our bound explodes at a certain utilization level
which is a function of the hop count. We show that for a general
network configuration and larger utilization utilization an upper
bound on delay, if it exists, must be a function of the number of
nodes and/or the number of flows in the network.",
KEYWORDS="Network Calculus; Delay Bound; Differentiated Services",
URL="http://dscwww.epfl.ch./EN/publications/ps_files/tr00_022.ps",
ENTRYBY=mis
}

