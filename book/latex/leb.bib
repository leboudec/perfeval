@TechReport{slef-infocom-submitted,
   abstract    = {We define a self-limiting epidemic service as a
                 dissemination service for ad-hoc environments that is
                 broadcast in nature, but is limited to a local scope
                 around each source. Example applications are chatting or
                 bulletin boards in a traffic jam, in an instant crowd in
                 a campus or, in contrast, along a desert highway. Our
                 goal is to support such a service across a wide range of
                 conditions (dense or sparse). The main problems are to
                 adaptively control scoping and traffic rates. We propose
                 a system design with the following elements: (1)
                 manipulation of TTL by adaptive aging mechanisms; (2)
                 control of forwarding factor by self-inhibition and
                 inter-inhibition and (3) control of rate of injection by
                 sources. We analyze our design using a mean field and
                 ordinary differential equation approach borrowed from
                 population dynamics, and show how it can be tuned to
                 achieve an appropriate balance between limitation of
                 scope and rate of information. We validate the design by
                 an implementation in Java. We assess the performance of
                 the design by analysis and by using the Java
                 implementation in a simulated MAC layer environment. Our
                 design is entirely self-organized, and is free of any
                 form of clustering or leader election.},
   author      = {El Fawal, Alaeddine and Le Boudec, Jean-Yves and Salamatian, Kave},
   details     = {http://infoscience.epfl.ch/search.py?recid=89512},
   documenturl = {http://infoscience.epfl.ch/getfile.py?recid=89512&mode=best},
   keywords    = {SLEF; Epidemic Forwarding; Congestion Control; Ad Hoc
                 Networks; Vehicular Networks},
   title       = {Self-{L}imiting {E}pidemic {F}orwarding},
   unit        = {LCA},
   institution =  "EPFL",
   number     = "LCA-REPORT-2006-126",
   year        = 2006
}
